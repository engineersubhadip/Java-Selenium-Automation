<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite thread-count="3" name="TestSuite" parallel="tests">

  <test name="Chrome">
  
  	<parameter name="browser" value="Chrome"/>
  	<parameter name="url" value="https://opensource-demo.orangehrmlive.com/web/index.php/auth/login"/>
  	
    <classes>
      <class name="dataProvidersAndParallelTesting.AssignmentEndtoEnd"/>
    </classes>
  </test>
  
  <test name="Edge">
  
  	<parameter name="browser" value="Edge"/>
  	<parameter name="url" value="https://opensource-demo.orangehrmlive.com/web/index.php/auth/login"/>
  	
    <classes>
      <class name="dataProvidersAndParallelTesting.AssignmentEndtoEnd"/>
    </classes>
  </test>
  
  <test name="Firefox">
  
  	<parameter name="browser" value="Firefox"/>
  	<parameter name="url" value="https://opensource-demo.orangehrmlive.com/web/index.php/auth/login"/>
  	
    <classes>
      <class name="dataProvidersAndParallelTesting.AssignmentEndtoEnd"/>
    </classes>
  </test>
  
  
</suite>
